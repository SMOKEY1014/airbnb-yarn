{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start-server": "node index.js",
    "start-client": "npm run dev --prefix ./client",
    "build-client": "npm run build --prefix ./client",
    "install-client": "npm install --prefix ./client",
    "build": "concurrently \"npm run install-client\" ",
    "start": "npm run build-client && npm run start-server",
    "install": "npm run install-client && npm install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "image-downloader": "^4.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.5",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
